{
  "version": 2,
  "builds": [
    {
      "src": "app.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/app.ts"
    },
    {
      "src": "/order-id",
      "dest": "/app.ts"
    },
    {
      "src": "/checkout-payment",
      "dest": "/app.ts"
    }
  ]
}
